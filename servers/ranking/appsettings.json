{
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000",
        "Protocols": "Http1AndHttp2"
      },
      "Https": {
        "Url": "https://0.0.0.0:5001",
        "Protocols": "Http1AndHttp2"
      }
    }
  },
  "RankingCache": {
    "TopExpirationSeconds": 3
  },
  "Logging": {
    "File": {
      "Path": "logs/ranking-.log"
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
